{
    "functions": {
      "api/server.py": {
        "runtime": "python3.9@0.4.0"
      }
    },
    "routes": [
      {
        "src": "/.*",
        "dest": "/api/server.py"
      }
    ],
    "builds": [
      {
        "src": "/api/server.py",
        "use": "@vercel/python"
      }
    ],
    "env": {
      "OPENAI_API_KEY": "@openai_api_key"
    }
  }
  