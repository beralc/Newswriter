{
    "version": 2,
    "name": "my-app-name",
    "env": {
      "SECRET_KEY": "@secret-key",
      "DATABASE_URL": "@database-url",
      "OPENAI_API_KEY": "@openai-api-key"
    },
    "functions": {
      "api/*.py": {
        "runtime": "python3.9"
      }
    },
    "routes": [
      { "src": "/(.*)", "dest": "/api/server.py" }
    ]
  }
  